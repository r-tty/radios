segment ABSOLUTE use32 at 0
ends

segment KCODE public use32 'code'
ends

segment KDATA public use32 'data'
ends

segment KVARS public use32 'vars'
ends

group KMEM KCODE,KDATA,KVARS
assume CS:KMEM, DS:KMEM, ES:KMEM, FS:KMEM, SS:KMEM
